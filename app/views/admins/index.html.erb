<div class="container">
  <table class="table">
    <thead>
      <th>Users Requiring an Intro</th>
      <th>Potential Intros</th>
      <th># of Intros to Date</th>
      <th>City</th>
    </thead>
    <tbody>
      <% @eligible_users.each do |user| %>
        <tr>
          <td><%= link_to user.name, user_path(user) %></td>
          <td><%= link_to 'Potential Intros', admin_path(user) %></td>
          <td><%= user.matches.count %> </td>
          <td><%= user.location.area %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
