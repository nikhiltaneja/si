<div class="container">
  <table class="table">
    <thead>
      <th>Users requiring a match</th>
    </thead>
    <tbody>
      <% @eligible_users.each do |user| %>
        <tr>
          <td><%= link_to user.name, admin_path(user) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
