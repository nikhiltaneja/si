<div class="container">
  <%= link_to 'Beta requests', admins_requests_path %>
  <%= link_to "Matches", admins_matches_path %>
  <%= link_to "Sidekiq", sidekiq_web_path %>
</div>

<div class="container">
  <table class="table">
    <thead>
      <th>Users requiring a match</th>
      <th>Potential Matches</th>
      <th># of Matches</th>
      <th>City</th>
    </thead>
    <tbody>
      <% @eligible_users.each do |user| %>
        <tr>
          <td><%= link_to user.name, user_path(user) %></td>
          <td><%= link_to 'Potential Matches', admin_path(user) %></td>
          <td><%= user.matches.count %> </td>
          <td><%= user.location.area %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
