<% if @shared_count > 0 %>
  <div class="row">
    <div class="col-md-4 col-md-offset-1 center">
      <h4># of Shared LinkedIn Connections: <%= @shared_count %></h4>
      <% @shared_connection_images.each do |connection| %>
        <%=image_tag connection[1], size: "35x35" %>
      <% end %>
    </div>
  </div>
<% end %>
